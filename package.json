{
  "name": "SmilesAR",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint --ext ts,tsx ./src",
    "lint:fix": "npm run lint -- --fix",
    "android_clean": "cd android && ./gradlew clean && cd ..",
    "ios_clean": "cd ios && pod deintegrate && pod cache clean --all && rm -rf Podfile.lock && cd ..",
    "remove_node_modules": "rm -rf node_modules",
    "clean": "npm run android_clean && npm run ios_clean && npm run remove_node_modules && yarn && npm run installpods",
    "installpods": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "18.5.0",
    "@react-native-firebase/app": "18.5.0",
    "@react-native-firebase/crashlytics": "18.5.0",
    "@react-native-firebase/perf": "18.5.0",
    "@react-navigation/native": "6.1.8",
    "@react-navigation/native-stack": "6.9.14",
    "@reduxjs/toolkit": "1.9.7",
    "@smiles-ar/services": "https://github.com/adonaipinheirosmiles/SmilesARServices.git",
    "formik": "2.4.5",
    "i18next": "23.5.1",
    "react": "18.2.0",
    "react-i18next": "13.2.2",
    "react-native": "0.72.5",
    "react-native-config": "1.5.1",
    "react-native-mmkv": "2.10.2",
    "react-native-safe-area-context": "4.7.2",
    "react-native-screens": "3.25.0",
    "react-native-smiles-ar-uikit": "0.3.1",
    "react-redux": "8.1.3",
    "yup": "1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
